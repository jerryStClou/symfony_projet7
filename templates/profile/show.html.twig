{% extends 'base.html.twig' %}

{% block title %}User
{% endblock %}

{% block body %}
	<h1>User</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ user.id }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ user.email }}</td>
			</tr>


			<tr>
				<th>Nom</th>
				<td>{{ user.nom }}</td>
			</tr>
			<tr>
				<th>Prenom</th>
				<td>{{ user.prenom }}</td>
			</tr>
			<tr>
				<th>Civilite</th>
				<td>{{ user.civilite }}</td>
			</tr>
			<tr>
				<th>Pseudo</th>
				<td>{{ user.pseudo }}</td>
			</tr>


		</tbody>
	</table>

	<a href="{{ path('app_profile_index') }}">back to list</a>

	<a href="{{ path('app_profile_edit', {'id': user.id}) }}">edit</a>

	{{ include('profile/_delete_form.html.twig') }}
{% endblock %}
